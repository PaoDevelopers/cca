{
	"$schema": "https://json.schemastore.org/prettierrc",
	"useTabs": true,
	"tabWidth": 8,
	"singleQuote": false,
	"semi": false,
	"endOfLine": "lf",
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
		{
			"files": [
				"*.js",
				"*.jsx",
				"*.ts",
				"*.tsx",
				"*.html",
				"*.svelte"
			],
			"options": {
				"tabWidth": 4
			}
		}
	]
}
